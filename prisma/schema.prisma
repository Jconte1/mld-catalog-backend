// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model products {
  id        String   @id @default(uuid())
  model     String?
  brand     String?
  type      String?
  features  String[]
  major     String?
  minor     String?
  data      Json
  created_at DateTime @default(now())
}
